#include <iostream>
using namespace std;

bool esPrimo(int num) {
    if (num <= 1) return false;
    for (int i = 2; i <= num / 2; i++) {
        if (num % i == 0) return false;
    }
    return true;
}

int main() {
    int n, contador = 0, num = 2;
    cout << "Ingrese la cantidad de nÃºmeros primos a imprimir: ";
    cin >> n;

    while (contador < n) {
        if (esPrimo(num)) {
            cout << num << " ";
            contador++;
        }
        num++;
    }
    cout << endl;
    return 0;
}
