#include <iostream>
#include <unordered_set>
#include <vector>
using namespace std;

bool wordBreak(string s, unordered_set<string>& dict){
    int n=s.size();
    vector<bool> dp(n+1,false);
    dp[0]=true;
    for(int i=1;i<=n;i++)
        for(int j=0;j<i;j++)
            if(dp[j] && dict.count(s.substr(j,i-j))){
                dp[i]=true;
                break;
            }
    return dp[n];
}

int main(){
    unordered_set<string> dict={"leet","code"};
    string s="leetcode";
    cout<<(wordBreak(s,dict)?"SÃ­ se puede segmentar":"No se puede segmentar")<<endl;
    return 0;
}
