#include <iostream>
#include <vector>
#include <queue>
using namespace std;

void topoKahn(vector<vector<int>>& adj){
    int n=adj.size();
    vector<int> indeg(n,0);
    for(int i=0;i<n;i++)
        for(int v: adj[i]) indeg[v]++;
    queue<int> q;
    for(int i=0;i<n;i++) if(indeg[i]==0) q.push(i);
    cout<<"Topological Order: ";
    while(!q.empty()){
        int u=q.front(); q.pop();
        cout<<u<<" ";
        for(int v: adj[u]){
            indeg[v]--;
            if(indeg[v]==0) q.push(v);
        }
    }
    cout<<endl;
}

int main(){
    vector<vector<int>> adj={{1,2},{3},{3},{4},{}};
    topoKahn(adj);
    return 0;
}
