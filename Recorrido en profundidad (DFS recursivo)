#include <iostream>
#include <vector>
using namespace std;

void DFSUtil(int v, vector<vector<int>>& adj, vector<bool>& visited){
    visited[v]=true;
    cout<<v<<" ";
    for(int u: adj[v])
        if(!visited[u]) DFSUtil(u, adj, visited);
}

void DFS(vector<vector<int>>& adj, int start){
    vector<bool> visited(adj.size(), false);
    DFSUtil(start, adj, visited);
}

int main(){
    vector<vector<int>> adj={{1,2},{0,3},{0,3},{1,2}};
    cout<<"DFS desde 0: ";
    DFS(adj,0);
    cout<<endl;
    return 0;
}
