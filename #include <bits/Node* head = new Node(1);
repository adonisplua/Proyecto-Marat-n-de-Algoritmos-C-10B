#include <bits/stdc++.h>
using namespace std;
struct Node { int val; Node* next; Node(int v):val(v),next(nullptr){}; };
int main() {
    Node* head = new Node(1);
    head->next = new Node(2);
    head->next->next = new Node(3);
    Node* cur = head;
    while(cur) { cout << cur->val << " "; cur = cur->next; }
    cout << "\n";
}
