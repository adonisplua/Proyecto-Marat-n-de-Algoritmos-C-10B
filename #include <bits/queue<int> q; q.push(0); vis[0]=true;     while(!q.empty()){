#include <bits/stdc++.h>
using namespace std;
int main(){
    vector<vector<int>> g={{1,2},{0,3},{0,4},{1},{2}};
    vector<bool> vis(5,false);
    queue<int> q; q.push(0); vis[0]=true;
    while(!q.empty()){
        int u=q.front(); q.pop();
        cout<<u<<" ";
        for(int v:g[u]) if(!vis[v]){ q.push(v); vis[v]=true; }
    }
    cout<<"\n";
}
