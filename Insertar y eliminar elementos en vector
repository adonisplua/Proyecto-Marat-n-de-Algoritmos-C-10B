#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> v = {10, 20, 30};
    v.push_back(40);
    v.pop_back();
    v.insert(v.begin()+1, 15); // insertar en la posición 1
    v.erase(v.begin()+2);      // eliminar posición 2
    for(int x : v) cout << x << " ";
    cout << endl;
    return 0;
}
