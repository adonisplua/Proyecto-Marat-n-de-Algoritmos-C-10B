void radix_sort(vector<int>& a) {
    int n = (int)a.size();
    int mx = 0;
    for (int x : a) mx = max(mx, x);
    vector<int> tmp(n);
    for (int exp = 1; mx/exp > 0; exp *= 10) {
        vector<int> cnt(10, 0);
        for (int x : a) cnt[(x/exp)%10]++;
        for (int i = 1; i < 10; ++i) cnt[i] += cnt[i-1];
        for (int i = n-1; i >= 0; --i) {
            int d = (a[i]/exp)%10;
            tmp[--cnt[d]] = a[i];
        }
        a = tmp;
    }
}
