#include <iostream>
#include <vector>
#include <climits>
using namespace std;

int primMatrix(vector<vector<int>>& graph){
    int n=graph.size();
    vector<int> key(n,INT_MAX); vector<bool> inMST(n,false);
    key[0]=0; int total=0;

    for(int count=0; count<n-1; count++){
        int u=-1;
        for(int i=0;i<n;i++)
            if(!inMST[i] && (u==-1 || key[i]<key[u])) u=i;
        inMST[u]=true;
        total+=key[u];
        for(int v=0;v<n;v++)
            if(graph[u][v] && !inMST[v] && graph[u][v]<key[v])
                key[v]=graph[u][v];
    }
    return total;
}

int main(){
    vector<vector<int>> graph={
        {0,2,0,6,0},
        {2,0,3,8,5},
        {0,3,0,0,7},
        {6,8,0,0,9},
        {0,5,7,9,0}};
    cout<<"Costo MST (Prim matriz): "<<primMatrix(graph)<<endl;
    return 0;
}
