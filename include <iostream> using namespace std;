include <iostream>
using namespace std;

int main() {
    int n;
    unsigned long long factorial = 1;

    cout << "Ingrese un número para calcular su factorial: ";
    cin >> n;

    if (n < 0) {
        cout << "No se puede calcular el factorial de un número negativo." << endl;
    } else {
        for (int i = 1; i <= n; i++) {
            factorial *= i;
        }
        cout << "Factorial de " << n << " es: " << factorial << endl;
    }
    return 0;
}
