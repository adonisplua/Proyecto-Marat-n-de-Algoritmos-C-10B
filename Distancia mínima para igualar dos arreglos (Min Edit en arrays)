#include <iostream>
#include <vector>
using namespace std;

int minOps(vector<int>& A, vector<int>& B){
    int n=A.size(), m=B.size();
    vector<vector<int>> dp(n+1, vector<int>(m+1,0));
    for(int i=1;i<=n;i++)
        for(int j=1;j<=m;j++)
            if(A[i-1]==B[j-1]) dp[i][j]=dp[i-1][j-1]+1;
            else dp[i][j]=max(dp[i-1][j], dp[i][j-1]);
    return n+m-2*dp[n][m];
}

int main(){
    vector<int> A={1,2,3,4}, B={3,4,5};
    cout<<"Operaciones mÃ­nimas: "<<minOps(A,B)<<endl;
    return 0;
}
