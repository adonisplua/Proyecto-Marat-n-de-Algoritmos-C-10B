#include <iostream>
using namespace std;

void swapXOR(int &a, int &b){
    if(a!=b){
        a=a^b;
        b=a^b;
        a=a^b;
    }
}

void bubbleSortXOR(int arr[], int n){
    for(int i=0;i<n-1;i++)
        for(int j=0;j<n-i-1;j++)
            if(arr[j]>arr[j+1]) swapXOR(arr[j],arr[j+1]);
}

int main(){
    int arr[]={5,3,2,4,1};
    int n=5;
    bubbleSortXOR(arr,n);
    for(int i=0;i<n;i++) cout<<arr[i]<<" ";
    return 0;
}
