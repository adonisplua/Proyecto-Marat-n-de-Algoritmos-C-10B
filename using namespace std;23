#include <bits/stdc++.h>
using namespace std;
struct FenwickTree{
    vector<int> bit; int n;
    FenwickTree(int size){ n=size; bit.assign(n+1,0); }
    void update(int idx,int val){ for(;idx<=n;idx+=idx&-idx) bit[idx]+=val; }
    int query(int idx){ int res=0; for(;idx>0;idx-=idx&-idx) res+=bit[idx]; return res; }
    int range(int l,int r){ return query(r)-query(l-1); }
};
int main(){
    FenwickTree ft(5); vector<int> a={1,2,3,4,5};
    for(int i=0;i<5;i++) ft.update(i+1,a[i]);
    cout<<ft.range(2,4)<<"\n"; // suma indices 2 a 4
}
