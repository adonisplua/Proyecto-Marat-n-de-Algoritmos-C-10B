#include <bits/stdc++.h>
using namespace std;

int main() {
    int n=6;
    vector<vector<int>> g(n);
    g[5]={2,0}; g[4]={0,1}; g[2]={3}; g[3]={1};
    vector<int> indeg(n,0);
    for(auto &v:g) for(int u:v) indeg[u]++;
    queue<int> q;
    for(int i=0;i<n;i++) if(indeg[i]==0) q.push(i);
    while(!q.empty()){
        int u=q.front(); q.pop();
        cout<<u<<" ";
        for(int v:g[u]) if(--indeg[v]==0) q.push(v);
    }
    cout<<"\n";
}
