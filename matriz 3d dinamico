#include <iostream>
using namespace std;

int main() {
    int x = 2, y = 2, z = 2;
    int*** cubo = new int**[x];

    for (int i = 0; i < x; i++) {
        cubo[i] = new int*[y];
        for (int j = 0; j < y; j++)
            cubo[i][j] = new int[z];
    }

    int val = 1;
    for (int i = 0; i < x; i++)
        for (int j = 0; j < y; j++)
            for (int k = 0; k < z; k++)
                cubo[i][j][k] = val++;

    for (int i = 0; i < x; i++)
        for (int j = 0; j < y; j++)
            for (int k = 0; k < z; k++)
                cout << "cubo[" << i << "][" << j << "][" << k << "] = "
                     << cubo[i][j][k] << endl;

    for (int i = 0; i < x; i++) {
        for (int j = 0; j < y; j++)
            delete[] cubo[i][j];
        delete[] cubo[i];
    }
    delete[] cubo;

    return 0;
}
