#include <iostream>
using namespace std;

int main() {
    int base, exponente;
    unsigned long long resultado = 1;
    cout << "Ingrese base y exponente: ";
    cin >> base >> exponente;

    for (int i = 1; i <= exponente; i++) {
        resultado *= base;
    }

    cout << base << " elevado a " << exponente << " es " << resultado << endl;
    return 0;
}
