#include <iostream>
#include <vector>
using namespace std;

void addEdge(vector<vector<int>>& adj, int u, int v){
    adj[u].push_back(v);
    adj[v].push_back(u); // no dirigido
}

void printGraph(vector<vector<int>>& adj){
    for(int i=0;i<adj.size();i++){
        cout<<"Nodo "<<i<<": ";
        for(int v: adj[i]) cout<<v<<" ";
        cout<<endl;
    }
}

int main(){
    int V=5;
    vector<vector<int>> adj(V);
    addEdge(adj,0,1);
    addEdge(adj,0,4);
    addEdge(adj,1,2);
    addEdge(adj,1,3);
    addEdge(adj,1,4);
    printGraph(adj);
    return 0;
}
