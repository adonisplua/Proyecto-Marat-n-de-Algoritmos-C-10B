#include <iostream>
#include <vector>
#include <queue>
using namespace std;

bool pathExists(vector<vector<int>>& adj, int start, int end){
    vector<bool> vis(adj.size(),false);
    queue<int> q;
    q.push(start); vis[start]=true;
    while(!q.empty()){
        int v=q.front(); q.pop();
        if(v==end) return true;
        for(int u: adj[v])
            if(!vis[u]){
                vis[u]=true;
                q.push(u);
            }
    }
    return false;
}

int main(){
    vector<vector<int>> adj={{1,2},{0,3},{0,3},{1,2}};
    cout<<"¿Camino 0→3? "<<(pathExists(adj,0,3)?"Sí":"No")<<endl;
    return 0;
}
