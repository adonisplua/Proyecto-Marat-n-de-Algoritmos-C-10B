#include <iostream>
using namespace std;

bool canPartition(int arr[], int n){
    int sum=0;
    for(int i=0;i<n;i++) sum+=arr[i];
    if(sum%2!=0) return false;
    int target=sum/2;
    bool dp[n+1][target+1];
    for(int i=0;i<=n;i++) dp[i][0]=true;
    for(int j=1;j<=target;j++) dp[0][j]=false;
    for(int i=1;i<=n;i++)
        for(int j=1;j<=target;j++)
            dp[i][j]=(arr[i-1]<=j)? dp[i-1][j]||dp[i-1][j-arr[i-1]] : dp[i-1][j];
    return dp[n][target];
}

int main(){
    int arr[]={1,5,11,5};
    cout<<"¿Se puede dividir en dos subconjuntos iguales? "<<(canPartition(arr,4)?"Sí":"No")<<endl;
    return 0;
}
