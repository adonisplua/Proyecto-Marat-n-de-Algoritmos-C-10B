#include <iostream>
#include <map>
#include <vector>
#include <algorithm>
using namespace std;

void relativeSort(int arr1[], int n1, int arr2[], int n2){
    map<int,int> freq;
    for(int i=0;i<n1;i++) freq[arr1[i]]++;
    int idx=0;
    for(int i=0;i<n2;i++)
        while(freq[arr2[i]]--){
            arr1[idx++]=arr2[i];
        }
    for(auto &p:freq)
        while(p.second--) arr1[idx++]=p.first;
}

int main(){
    int arr1[]={2,1,2,5,7,1,9,3,6,8,8};
    int arr2[]={2,1,8,3};
    int n1=11, n2=4;
    relativeSort(arr1,n1,arr2,n2);
    for(int i=0;i<n1;i++) cout<<arr1[i]<<" ";
    return 0;
}
