#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<int> v = {1,2,4,4,5,6};
    auto lb = lower_bound(v.begin(), v.end(), 4); // primer >= 4
    auto ub = upper_bound(v.begin(), v.end(), 4); // primer > 4
    cout << "lower_bound: " << distance(v.begin(), lb) << endl;
    cout << "upper_bound: " << distance(v.begin(), ub) << endl;
    return 0;
}
