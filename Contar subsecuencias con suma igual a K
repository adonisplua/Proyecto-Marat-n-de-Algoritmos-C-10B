#include <iostream>
#include <vector>
using namespace std;

int countSubsetsSum(vector<int>& arr, int n, int sum){
    vector<vector<int>> dp(n+1, vector<int>(sum+1,0));
    for(int i=0;i<=n;i++) dp[i][0]=1;
    for(int i=1;i<=n;i++)
        for(int j=1;j<=sum;j++)
            if(arr[i-1]<=j)
                dp[i][j]=dp[i-1][j]+dp[i-1][j-arr[i-1]];
            else dp[i][j]=dp[i-1][j];
    return dp[n][sum];
}

int main(){
    vector<int> arr={2,3,5,6,8,10};
    cout<<"Subconjuntos con suma 10: "<<countSubsetsSum(arr,arr.size(),10)<<endl;
    return 0;
}
