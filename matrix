#include <iostream>
using namespace std;

int main() {
    int filas = 3, cols = 3;
    int** mat = new int*[filas];
    for (int i = 0; i < filas; i++)
        mat[i] = new int[cols];

    for (int i = 0; i < filas; i++)
        for (int j = 0; j < cols; j++)
            mat[i][j] = i + j;

    for (int i = 0; i < filas; i++) {
        for (int j = 0; j < cols; j++)
            cout << mat[i][j] << " ";
        cout << endl;
    }

    for (int i = 0; i < filas; i++)
        delete[] mat[i];
    delete[] mat;

    return 0;
}
