#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<int> v = {1,2,3,4,5};
    vector<int> res(v.size());
    transform(v.begin(), v.end(), res.begin(), [](int x){ return x*x; }); // cuadrado
    for(int x : res) cout << x << " ";
    cout << endl;
    return 0;
}
