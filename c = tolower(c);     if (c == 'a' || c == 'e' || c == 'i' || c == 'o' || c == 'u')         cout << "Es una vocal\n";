#include <iostream>
using namespace std;

int main() {
    char c;
    cout << "Ingrese un carÃ¡cter: ";
    cin >> c;

    c = tolower(c);
    if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u')
        cout << "Es una vocal\n";
    else if ((c >= 'b' && c <= 'z'))
        cout << "Es una consonante\n";
    else
        cout << "No es una letra\n";

    return 0;
}

