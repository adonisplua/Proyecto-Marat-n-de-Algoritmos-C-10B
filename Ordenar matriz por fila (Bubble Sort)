#include <iostream>
using namespace std;

void bubbleSortRow(int row[], int n){
    for(int i=0;i<n-1;i++)
        for(int j=0;j<n-i-1;j++)
            if(row[j]>row[j+1]) swap(row[j],row[j+1]);
}

int main(){
    int mat[2][3]={{3,1,2},{6,5,4}};
    for(int i=0;i<2;i++){
        bubbleSortRow(mat[i],3);
    }
    for(int i=0;i<2;i++){
        for(int j=0;j<3;j++) cout<<mat[i][j]<<" ";
        cout<<endl;
    }
    return 0;
}
