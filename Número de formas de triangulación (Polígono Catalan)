#include <iostream>
#include <vector>
using namespace std;

int numTriangulations(int n){
    vector<long long> dp(n+1,0);
    dp[2]=1;
    for(int i=3;i<=n;i++)
        for(int k=1;k<i-1;k++)
            dp[i]+=dp[k+1]*dp[i-k];
    return dp[n];
}

int main(){
    int n=5;
    cout<<"NÃºmero de triangulaciones posibles: "<<numTriangulations(n)<<endl;
    return 0;
}
