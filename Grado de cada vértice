#include <iostream>
#include <vector>
using namespace std;

void printDegrees(vector<vector<int>>& adj){
    for(int i=0;i<adj.size();i++)
        cout<<"Grado("<<i<<") = "<<adj[i].size()<<endl;
}

int main(){
    vector<vector<int>> adj={{1,2},{0,3},{0,3},{1,2}};
    printDegrees(adj);
    return 0;
}
