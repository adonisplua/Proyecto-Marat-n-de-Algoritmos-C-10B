#include <iostream>
#include <string>
using namespace std;

void selectionSortStrings(string arr[], int n){
    for(int i=0;i<n-1;i++){
        int min_idx=i;
        for(int j=i+1;j<n;j++)
            if(arr[j]<arr[min_idx]) min_idx=j;
        swap(arr[i],arr[min_idx]);
    }
}

int main(){
    string arr[]={"perro","gato","elefante","abeja"};
    int n=4;
    selectionSortStrings(arr,n);
    for(int i=0;i<n;i++) cout<<arr[i]<<" ";
    return 0;
}
