#include <iostream>
using namespace std;

struct Nodo {
    int dato;
    Nodo* siguiente;
};

struct Cola {
    Nodo* frente;
    Nodo* fin;
};

void encolar(Cola& c, int valor) {
    Nodo* nuevo = new Nodo{valor, nullptr};
    if (!c.frente)
        c.frente = c.fin = nuevo;
    else {
        c.fin->siguiente = nuevo;
        c.fin = nuevo;
    }
}

int desencolar(Cola& c) {
    if (!c.frente) return -1;
    Nodo* temp = c.frente;
    int valor = temp->dato;
    c.frente = c.frente->siguiente;
    if (!c.frente) c.fin = nullptr;
    delete temp;
    return valor;
}

int main() {
    Cola c = {nullptr, nullptr};
    encolar(c, 10);
    encolar(c, 20);
    cout << "Desencolado: " << desencolar(c) << endl;
    cout << "Desencolado: " << desencolar(c) << endl;
    return 0;
}
