#include <bits/stdc++.h>
using namespace std;
struct Item{ int val,wt; };
bool cmp(Item a,Item b){ return (double)a.val/a.wt > (double)b.val/b.wt; }
int main(){
    vector<Item> a={{60,10},{100,20},{120,30}};
    int W=50; sort(a.begin(),a.end(),cmp); double res=0;
    for(auto x:a){
        int take=min(x.wt,W);
        res+=take*(double)x.val/x.wt;
        W-=take;
        if(W==0) break;
    }
    cout<<res<<"\n";
}
