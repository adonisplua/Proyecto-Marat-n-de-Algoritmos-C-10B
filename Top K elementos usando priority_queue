#include <iostream>
#include <queue>
#include <vector>
using namespace std;

int main() {
    vector<int> v = {5,1,3,6,8,2};
    int k=3;
    priority_queue<int> pq(v.begin(),v.end());

    cout << "Top " << k << " elementos: ";
    for(int i=0;i<k;i++){
        cout << pq.top() << " ";
        pq.pop();
    }
    cout << endl;
    return 0;
}
