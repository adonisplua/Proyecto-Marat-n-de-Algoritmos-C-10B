#include <iostream>
using namespace std;

bool buscarElemento(int arr[], int n, int x) {
    if (n == 0) return false;
    if (arr[n - 1] == x) return true;
    return buscarElemento(arr, n - 1, x);
}

int main() {
    int arr[] = {1, 3, 5, 7, 9};
    int x = 5;
    cout << (buscarElemento(arr, 5, x) ? "Encontrado" : "No encontrado") << endl;
    return 0;
}
