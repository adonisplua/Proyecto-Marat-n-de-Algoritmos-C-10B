#include <iostream>
using namespace std;

int main() {
    int n, a = 0, b = 1, siguiente;
    cout << "Ingrese la cantidad de tÃ©rminos de Fibonacci: ";
    cin >> n;

    for (int i = 0; i < n; i++) {
        cout << a << " ";
        siguiente = a + b;
        a = b;
        b = siguiente;
    }
    cout << endl;
    return 0;
}
