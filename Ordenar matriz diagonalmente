#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

void ordenarDiagonal(int mat[][3], int n){
    vector<int> diag;
    for(int i=0;i<n;i++) diag.push_back(mat[i][i]);
    sort(diag.begin(), diag.end());
    for(int i=0;i<n;i++) mat[i][i]=diag[i];
}

int main(){
    int mat[3][3]={{3,2,1},{6,5,4},{9,8,7}};
    ordenarDiagonal(mat,3);
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++) cout<<mat[i][j]<<" ";
        cout<<endl;
    }
    return 0;
}
