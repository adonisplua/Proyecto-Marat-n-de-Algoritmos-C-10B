#include <iostream>
#include <cmath>
using namespace std;

class Punto {
private:
    float x, y;
public:
    Punto(float x, float y) : x(x), y(y) {}
    double distancia(Punto otro) {
        return sqrt(pow(x - otro.x, 2) + pow(y - otro.y, 2));
    }
};

int main() {
    Punto p1(1, 1), p2(4, 5);
    cout << "Distancia: " << p1.distancia(p2) << endl;
    return 0;
}
