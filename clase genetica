#include <iostream>
using namespace std;

template <typename T>
class Caja {
private:
    T valor;
public:
    Caja(T v) : valor(v) {}
    void mostrar() { cout << "Contenido: " << valor << endl; }
    T obtener() { return valor; }
};

int main() {
    Caja<int> c1(42);
    Caja<string> c2("Hola Mundo");

    c1.mostrar();
    c2.mostrar();
    return 0;
}
