#include <iostream>
using namespace std;

int* memoriaGlobal = nullptr;

void inicializar() {
    memoriaGlobal = new int[5]{10, 20, 30, 40, 50};
}

void mostrar() {
    for (int i = 0; i < 5; i++)
        cout << memoriaGlobal[i] << " ";
    cout << endl;
}

void liberar() {
    delete[] memoriaGlobal;
    memoriaGlobal = nullptr;
}

int main() {
    inicializar();
    mostrar();
    liberar();
    return 0;
}
