#include <iostream>
using namespace std;

void stoogeSort(int arr[], int l, int h){
    if(arr[l]>arr[h]) swap(arr[l],arr[h]);
    if(h-l+1>2){
        int t=(h-l+1)/3;
        stoogeSort(arr,l,h-t);
        stoogeSort(arr,l+t,h);
        stoogeSort(arr,l,h-t);
    }
}

int main(){
    int arr[]={5,6,7,2,1,0};
    int n=6;
    stoogeSort(arr,0,n-1);
    for(int i=0;i<n;i++) cout<<arr[i]<<" ";
    return 0;
}
