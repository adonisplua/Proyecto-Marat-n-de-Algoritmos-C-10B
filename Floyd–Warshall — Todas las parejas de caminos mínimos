#include <iostream>
#include <vector>
#include <climits>
using namespace std;

void floydWarshall(vector<vector<int>>& dist){
    int n=dist.size();
    for(int k=0;k<n;k++)
        for(int i=0;i<n;i++)
            for(int j=0;j<n;j++)
                if(dist[i][k]!=INT_MAX && dist[k][j]!=INT_MAX)
                    dist[i][j]=min(dist[i][j], dist[i][k]+dist[k][j]);

    cout<<"Matriz de distancias mÃ­nimas:\n";
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++)
            cout<<(dist[i][j]==INT_MAX?999:dist[i][j])<<" ";
        cout<<endl;
    }
}

int main(){
    const int INF=INT_MAX;
    vector<vector<int>> dist={
        {0,5,INF,10},
        {INF,0,3,INF},
        {INF,INF,0,1},
        {INF,INF,INF,0}
    };
    floydWarshall(dist);
    return 0;
}
