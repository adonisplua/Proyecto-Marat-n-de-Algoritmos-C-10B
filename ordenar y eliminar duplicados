#include <iostream>
#include <set>
using namespace std;

int main() {
    int arr[] = {3, 1, 2, 3, 2, 4};
    int n = sizeof(arr)/sizeof(arr[0]);
    set<int> s;
    for(int i=0;i<n;i++) s.insert(arr[i]); // elimina duplicados automÃ¡ticamente
    
    cout << "Arreglo ordenado sin duplicados: ";
    for(int x : s) cout << x << " ";
    return 0;
}
