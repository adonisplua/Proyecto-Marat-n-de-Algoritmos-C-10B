int binary_search_rec_helper(const vector<long long>& a, int l, int r, long long key) {
    if (l > r) return -1;
    int m = l + (r - l) / 2;
    if (a[m] == key) return m;
    if (a[m] < key) return binary_search_rec_helper(a, m+1, r, key);
    else return binary_search_rec_helper(a, l, m-1, key);
}
int binary_search_rec(const vector<long long>& a, long long key) {
    return binary_search_rec_helper(a, 0, (int)a.size()-1, key);
}
