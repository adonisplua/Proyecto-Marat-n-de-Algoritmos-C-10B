vector<long long> k_smallest_from_two_sorted(const vector<long long>& A, const vector<long long>& B, int k) {
    vector<long long> res;
    int i=0,j=0;
    while ((int)res.size() < k && (i < (int)A.size() || j < (int)B.size())) {
        if (i < (int)A.size() && (j >= (int)B.size() || A[i] <= B[j])) res.push_back(A[i++]);
        else res.push_back(B[j++]);
    }
    return res;
}
