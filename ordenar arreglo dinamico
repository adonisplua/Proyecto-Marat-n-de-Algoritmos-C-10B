#include <iostream>
using namespace std;

void ordenar(int* arr, int n) {
    for (int i = 0; i < n-1; i++)
        for (int j = i+1; j < n; j++)
            if (*(arr+j) < *(arr+i))
                swap(*(arr+i), *(arr+j));
}

int main() {
    int n = 5;
    int* arr = new int[n]{5, 3, 4, 1, 2};
    ordenar(arr, n);

    for (int i = 0; i < n; i++)
        cout << arr[i] << " ";

    delete[] arr;
    return 0;
}
