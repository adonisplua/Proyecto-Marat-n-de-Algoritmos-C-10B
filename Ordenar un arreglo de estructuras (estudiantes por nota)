#include <iostream>
using namespace std;

struct Estudiante {
    string nombre;
    int nota;
};

void ordenarPorNota(Estudiante arr[], int n){
    for(int i=0;i<n-1;i++){
        int max_idx=i;
        for(int j=i+1;j<n;j++)
            if(arr[j].nota>arr[max_idx].nota) max_idx=j;
        swap(arr[i], arr[max_idx]);
    }
}

int main(){
    Estudiante arr[]={{"Ana",90},{"Luis",75},{"Marta",85}};
    int n=3;
    ordenarPorNota(arr,n);
    for(int i=0;i<n;i++) cout<<arr[i].nombre<<" "<<arr[i].nota<<endl;
    return 0;
}
