#include <iostream>
#include <vector>
using namespace std;

bool isEulerian(vector<vector<int>>& adj){
    int odd=0;
    for(int i=0;i<adj.size();i++)
        if(adj[i].size()%2!=0) odd++;
    if(odd==0) return true; // Euleriano
    if(odd==2) return true; // Semi-euleriano
    return false;
}

int main(){
    vector<vector<int>> adj={{1,2},{0,2},{0,1,3},{2}};
    cout<<"¿Tiene camino/circuito euleriano? "<<(isEulerian(adj)?"Sí":"No")<<endl;
    return 0;
}
