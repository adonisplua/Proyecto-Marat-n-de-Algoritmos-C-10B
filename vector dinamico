#include <iostream>
#include <cstring>
using namespace std;

int main() {
    int n = 3;
    char** nombres = new char*[n];
    nombres[0] = strdup("Ana");
    nombres[1] = strdup("Luis");
    nombres[2] = strdup("Marta");

    for (int i = 0; i < n; i++)
        cout << nombres[i] << endl;

    for (int i = 0; i < n; i++)
        free(nombres[i]);
    delete[] nombres;

    return 0;
}
