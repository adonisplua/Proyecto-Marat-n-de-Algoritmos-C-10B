#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<pair<int,int>> vp = {{1,3},{2,2},{3,1}};
    sort(vp.begin(), vp.end(), [](pair<int,int> a, pair<int,int> b){
        return a.second < b.second;
    });

    for(auto [x,y]: vp) cout << "(" << x << "," << y << ") ";
    cout << endl;
    return 0;
}
