#include <iostream>
#include <queue>
#include <vector>
using namespace std;

int main() {
    priority_queue<int> maxHeap; // por defecto
    priority_queue<int, vector<int>, greater<int>> minHeap;

    maxHeap.push(5); maxHeap.push(2); maxHeap.push(8);
    minHeap.push(5); minHeap.push(2); minHeap.push(8);

    cout << "Max heap: ";
    while(!maxHeap.empty()){ cout << maxHeap.top() << " "; maxHeap.pop();}
    cout << "\nMin heap: ";
    while(!minHeap.empty()){ cout << minHeap.top() << " "; minHeap.pop();}
    cout << endl;
    return 0;
}
