#include <iostream>
#include <string>
using namespace std;

void ordenarPorLongitud(string arr[], int n){
    for(int i=0;i<n-1;i++)
        for(int j=0;j<n-i-1;j++)
            if(arr[j].length()>arr[j+1].length()) swap(arr[j],arr[j+1]);
}

int main(){
    string arr[]={"gato","elefante","perro","raton"};
    int n=4;
    ordenarPorLongitud(arr,n);
    for(int i=0;i<n;i++) cout<<arr[i]<<" ";
    return 0;
}
