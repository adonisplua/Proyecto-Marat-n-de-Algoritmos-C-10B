#include <iostream>
#include <vector>
using namespace std;

int countBinaryStrings(int n){
    vector<int> a(n+1), b(n+1);
    a[1]=b[1]=1;
    for(int i=2;i<=n;i++){
        a[i]=a[i-1]+b[i-1];
        b[i]=a[i-1];
    }
    return a[n]+b[n];
}

int main(){
    cout<<"Cadenas binarias sin '11' de longitud 5: "<<countBinaryStrings(5)<<endl;
    return 0;
}
